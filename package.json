{
	"name": "svg-preview-in-code",
	"displayName": "Svg Preview In Code",
	"description": "Preview svg code inline.",
	"repository": {
		"url": "https://github.com/a1245582339/svg-preview-in-code"
	},
	"publisher": "lzp",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"commands": [
			{
				"command": "spic.gallery",
				"title": "SVG Gallery"
			}
		],
		"configuration": [
			{
				"title": "SvgPreviewInCode",
				"properties": {
					"spic.include": {
						"type": [
							"array",
							"null"
						],
						"default": null
					},
					"spic.exclude": {
						"type": [
							"array",
							"null"
						],
						"default": null
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./",
		"compile": "tsup && node copy-package.js",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"package": "npm run compile && cd out && vsce package",
		"publish": "npm run compile && cd out && vsce publish",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/react-dom": "^18.0.5",
		"@types/styled-components": "^5.1.25",
		"@types/vscode-webview": "^1.57.0",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"@vscode/vsce": "^3.6.0",
		"eslint": "^8.14.0",
		"fast-xml-parser": "^4.0.7",
		"glob": "^8.0.3",
		"mocha": "^9.2.2",
		"react-dom": "^18.1.0",
		"react": "^18.1.0",
		"styled-components": "^5.3.5",
		"tsup": "^8.5.0",
		"typescript": "^4.6.4"
	}
}
